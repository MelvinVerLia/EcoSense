{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "rewrites": [
        {
            "source": "/build/(.*)",
            "destination": "/public/build/$1"
        },
        {
            "source": "/images/(.*)",
            "destination": "/public/images/$1"
        },
        {
            "source": "/articles/(.*)",
            "destination": "/public/storage/articles/$1"
        },
        {
            "source": "/contributions/(.*)",
            "destination": "/public/storage/contributions/$1"
        },
        {
            "source": "/merchandises/(.*)",
            "destination": "/public/storage/merchandises/$1"
        },
        {
            "source": "/css/filament/(.*)",
            "destination": "/public/css/filament/$1"
        },
        {
            "source": "/js/filament/(.*)",
            "destination": "/public/js/filament/$1"
        }
    ],
    "public": true,
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://ecosense.com",
        "APP_KEY": "base64:GJf98RFHnK9qs87Z/uiLneOPazwO6KRgT7995tOwL7g=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "m3-qi.h.filess.io",
        "DB_PORT": "3307",
        "DB_DATABASE": "ecosense_campscene",
        "DB_USERNAME": "ecosense_campscene"
    }
}